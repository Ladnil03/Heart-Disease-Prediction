{
  "buildCommand": "echo 'Frontend and API ready'",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/index.html",
      "destination": "/web_frontend/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/web_frontend/:path*"
    }
  ]
}
