{
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "PYTHONPATH": "$PYTHONPATH:/var/task"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/web_frontend/$1"
    }
  ]
}
